<p id="notice"><%= notice %></p>

<h1>Learnings</h1>

<table>
  <thead>
    <tr>
      <th>Quality</th>
      <th>Studies</th>
      <th>Projects</th>
      <th>Investigation</th>
      <th>Infrastructure</th>
      <th>Experience</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @learnings.each do |learning| %>
      <tr>
        <td><%= learning.Quality %></td>
        <td><%= learning.Studies %></td>
        <td><%= learning.Projects %></td>
        <td><%= learning.Investigation %></td>
        <td><%= learning.Infrastructure %></td>
        <td><%= learning.Experience %></td>
        <td><%= link_to 'Show', learning %></td>
        <td><%= link_to 'Edit', edit_learning_path(learning) %></td>
        <td><%= link_to 'Destroy', learning, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Learning', new_learning_path %>
